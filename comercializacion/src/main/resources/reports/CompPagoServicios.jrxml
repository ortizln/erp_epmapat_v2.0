	<subDataset name="HistorialConsumo" uuid="d9d002d4-d5d1-4879-a6fb-864a91b89472">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ErpEpmapaT"/>
		<parameter name="cuenta" class="java.lang.Long"/>
		<queryString language="SQL">
			<![CDATA[SELECT 
    e.emision,
    l.lecturaactual - l.lecturaanterior AS m3
FROM lecturas l
JOIN emisiones e ON l.idemision = e.idemision 
WHERE 
    l.idabonado_abonados = $P{cuenta} AND l.observaciones IS NULL
    AND (l.lecturaactual - l.lecturaanterior) >= 0
ORDER BY l.idemision DESC
LIMIT 8]]>
		</queryString>
		<field name="emision" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="emision"/>
			<property name="com.jaspersoft.studio.field.label" value="emision"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="emisiones"/>
		</field>
		<field name="m3" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.name" value="m3"/>
			<property name="com.jaspersoft.studio.field.label" value="m3"/>
		</field>
		<group name="emision">
			<groupExpression><![CDATA[$F{emision}]]></groupExpression>
		</group>
		<group name="m3">
			<groupExpression><![CDATA[$F{m3}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="idfactura" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select
	f.nrofactura,
	f.idfactura,
	c.nombre,
	c.cedula,
	c.direccion,
	u.nomusu,
	m.descripcion as modulo,
	f.fechacobro
from
	 facturas f
join clientes c on
	f.idcliente = c.idcliente
join usuarios u on
	f.usuariocobro = u.idusuario
join modulos m on
	f.idmodulo = m.idmodulo
where
	f.idfactura= $P{idfactura}]]>
	</queryString>
