	<subDataset name="DetallesFactura" uuid="25e7cfb9-414f-486c-953b-ebd40ba49c33">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ErpEpmapaT"/>
		<parameter name="idfactura" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT r.descripcion,
	rf.cantidad,
	rf.valorunitario,
	rf.cantidad * rf.valorunitario AS total
FROM rubroxfac rf
	join rubros r ON 
	 rf.idrubro_rubros = r.idrubro 
WHERE 
	 rf.idfactura_facturas = $P{idfactura} 
	 AND NOT  ( 
	 rf.idrubro_rubros = 165 
	 OR rf.idrubro_rubros = 5)]]>
		</queryString>
