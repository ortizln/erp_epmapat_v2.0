	<subDataset name="FacFPCobros" uuid="7a0b6bcd-59e4-45b5-adf9-85de4e17f077">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ErpEpmapaT"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="desde" class="java.util.Date"/>
		<parameter name="hasta" class="java.util.Date"/>
		<parameter name="hdesde" class="java.sql.Time"/>
		<parameter name="hhasta" class="java.sql.Time"/>
		<parameter name="usuariocobro" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[-----
WITH iva_total AS (
    SELECT 
        f.formapago,
        SUM(ROUND(f.swiva::numeric, 2)) AS sumaiva
    FROM Facturas f
    WHERE 
        f.fechacobro BETWEEN $P{desde} AND $P{hasta}
        AND f.horacobro BETWEEN $P{hdesde} AND $P{hhasta}
        AND f.estado <> 3 and f.usuariocobro = $P{usuariocobro}
        AND f.fechaeliminacion IS NULL
        AND (f.fechaanulacion IS NULL)
    GROUP BY f.formapago
),

facturas_filtradas AS (
    SELECT 
        f.idfactura, 
        f.formapago,
        fc.descripcion, 
        COALESCE(f.valornotacredito, 0) AS valornotacredito,
        f.swcondonar
    FROM Facturas f
    JOIN Formacobro fc ON f.formapago = fc.idformacobro
    WHERE 
        f.fechacobro BETWEEN $P{desde} AND $P{hasta}
        AND f.horacobro BETWEEN $P{hdesde} AND $P{hhasta}
        AND f.estado <> 3  and f.usuariocobro = $P{usuariocobro}
        AND f.fechaeliminacion IS NULL 
        AND (f.fechaanulacion IS NULL OR f.fechaanulacion <= $P{desde})
)

SELECT 
    f.formapago,
    f.descripcion,
    COUNT(f.idfactura) AS nfacturas,
    
    ROUND(SUM(CASE 
        WHEN f.swcondonar IS TRUE AND rf.idrubro_rubros = 6 
        THEN 0 
        ELSE rf.valorunitario * rf.cantidad 
    END)::numeric, 2) AS subtotal,

    ROUND(it.sumaiva, 2) AS sumaiva,

    ROUND(SUM(f.valornotacredito)::numeric, 2) AS valornotacredito

FROM facturas_filtradas f
JOIN Rubroxfac rf ON f.idfactura = rf.idfactura_facturas
JOIN Rubros r ON r.idrubro = rf.idrubro_rubros
LEFT JOIN iva_total it ON f.formapago = it.formapago

GROUP BY 
    f.formapago, f.descripcion, it.sumaiva;]]>
		</queryString>
		<field name="formapago" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.name" value="formapago"/>
			<property name="com.jaspersoft.studio.field.label" value="formapago"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="facturas"/>
		</field>
		<field name="descripcion" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.name" value="descripcion"/>
			<property name="com.jaspersoft.studio.field.label" value="descripcion"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="formacobro"/>
		</field>
		<field name="nfacturas" class="java.lang.Long">
			<property name="com.jaspersoft.studio.field.name" value="nfacturas"/>
			<property name="com.jaspersoft.studio.field.label" value="nfacturas"/>
		</field>
		<field name="subtotal" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.name" value="subtotal"/>
			<property name="com.jaspersoft.studio.field.label" value="subtotal"/>
		</field>
		<field name="sumaiva" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.name" value="sumaiva"/>
			<property name="com.jaspersoft.studio.field.label" value="sumaiva"/>
		</field>
		<field name="valornotacredito" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.name" value="valornotacredito"/>
			<property name="com.jaspersoft.studio.field.label" value="valornotacredito"/>
		</field>
	</subDataset>
	<parameter name="desde" class="java.util.Date"/>
	<parameter name="hasta" class="java.util.Date"/>
	<parameter name="idusuario" class="java.lang.Integer"/>
	<parameter name="tope" class="java.util.Date"/>
	<parameter name="hdesde" class="java.sql.Time"/>
	<parameter name="hhasta" class="java.sql.Time"/>
	<parameter name="usuariocobro" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[WITH facturas_validas AS (
    SELECT DISTINCT f.idfactura, f.swiva
    FROM Facturas f
    WHERE f.fechacobro  BETWEEN $P{desde} AND $P{hasta}
      AND f.horacobro BETWEEN $P{hdesde} AND $P{hhasta}
      AND f.estado <> 3 and f.usuariocobro = $P{usuariocobro}
      AND f.fechaeliminacion IS NULL 
      AND (f.fechaanulacion IS NULL OR f.fechaanulacion <= $P{desde})
)

SELECT 
    COUNT(DISTINCT f.idfactura) AS nfacturas, 

ROUND(
    SUM(
        CASE 
            WHEN f.swcondonar IS TRUE AND rf.idrubro_rubros = 6 
            THEN 0 
            ELSE (rf.valorunitario * rf.cantidad)::numeric 
        END
    ), 2
) AS subtotal,


    (SELECT SUM(ROUND(swiva::numeric, 2)) FROM facturas_validas) AS sumaIva

FROM Rubroxfac rf 
JOIN Facturas f ON f.idfactura = rf.idfactura_facturas 
JOIN Rubros r ON r.idrubro = rf.idrubro_rubros 

WHERE 
    f.fechacobro BETWEEN $P{desde} AND $P{hasta}
    AND f.horacobro BETWEEN $P{hdesde} AND $P{hhasta}
    AND f.estado <> 3 and f.usuariocobro = $P{usuariocobro}
    AND f.fechaeliminacion IS NULL 
    AND (f.fechaanulacion IS NULL OR f.fechaanulacion <= $P{desde});]]>
	</queryString>
